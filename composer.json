{
    "name": "eonx-com/easy-core",
    "description": "Provides core features for Lumen applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4",
        "laravel/lumen-framework": "^5.5",
        "phpspec/prophecy": "~1.0",
        "api-platform/core": "^2.5",
        "doctrine/orm": "^2.6",
        "mockery/mockery": "^1.2",
        "symfony/symfony": "^4.2 || ^5.0",
        "symfony/maker-bundle": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyCore\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
